            <Font name="Arial" size="24"/>
        </font>
    </Label>

    <Label text="TotalExpenses" GridPane.columnIndex="0"
           GridPane.rowIndex="16" >
    </Label>
    <TextField fx:id="tfTotalExpenses" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="16"
               editable="false"/>

    <Label text="UnrecoveredRents" GridPane.columnIndex="0"
           GridPane.rowIndex="17" >
    </Label>
    <TextField fx:id="tfUnrecoveredRents" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="17"
               editable="false"/>

    <Button fx:id="btnGenerate" text="Generate" GridPane.halignment="CENTER" GridPane.rowIndex="18"/>
