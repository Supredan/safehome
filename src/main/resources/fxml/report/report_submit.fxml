<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.shape.Line?>
<GridPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" alignment="center"
          hgap="10" vgap="10"
          fx:controller="controllers.report.ReportSubmitController">
<!--    <padding><Insets top="40" right="40" bottom="40" left="40"/></padding>-->

    <!-- Add Header Label -->
    <Label text="Quarterly Report" GridPane.columnIndex="0"
           GridPane.rowIndex="0" GridPane.columnSpan="2"
           GridPane.rowSpan="1" GridPane.halignment="LEFT">
        <font>
            <Font name="Arial" size="24"/>
        </font>
    </Label>

    <Label text="Building#" GridPane.columnIndex="0"
           GridPane.rowIndex="1" >
    </Label>
    <TextField fx:id="tfBuilding" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="1"
               editable="true"/>

    <Label text="Address" GridPane.columnIndex="0"
           GridPane.rowIndex="2" >
    </Label>
    <TextField fx:id="tfAddress" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="2" editable="false"/>

    <Label text="Quarter" GridPane.columnIndex="0"
           GridPane.rowIndex="3" >
    </Label>
    <TextField fx:id="tfQuarter" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="3"/>


    <Label text="Year" GridPane.columnIndex="2"
           GridPane.rowIndex="3" >
    </Label>
    <TextField fx:id="tfYear" prefHeight="40"
               GridPane.columnIndex="3" GridPane.rowIndex="3"/>

    <Label text="TotalApartments" GridPane.columnIndex="0"
           GridPane.rowIndex="4" >
    </Label>
    <TextField fx:id="tfTotalApartments" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="4" editable="false"/>

    <Label text="CurrentlyOccupied" GridPane.columnIndex="2"
           GridPane.rowIndex="4" >
    </Label>
    <TextField fx:id="tfCurrentlyOccupied" prefHeight="40"
               GridPane.columnIndex="3" GridPane.rowIndex="4" editable="false"/>

    <Label text="Percent" GridPane.columnIndex="0"
           GridPane.rowIndex="5" >
    </Label>
    <TextField fx:id="tfPercent" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="5" editable="false"/>

    <Label text="No.changing tenants" GridPane.columnIndex="2"
           GridPane.rowIndex="5" >
    </Label>
    <TextField fx:id="tfChangingTenants" prefHeight="40"
               GridPane.columnIndex="3" GridPane.rowIndex="5" editable="false"/>


    <Label text="Revenues" GridPane.columnIndex="0"
           GridPane.rowIndex="6" GridPane.columnSpan="2"
           GridPane.rowSpan="1" GridPane.halignment="LEFT">
        <font>
            <Font name="Arial" size="24"/>
        </font>
    </Label>

    <Label text="TotalRentRevenue" GridPane.columnIndex="0"
           GridPane.rowIndex="7" >
    </Label>
    <TextField fx:id="tfTotalRentRevenue" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="7"
               editable="false"/>

    <Label text="Expenses" GridPane.columnIndex="0"
           GridPane.rowIndex="8" GridPane.columnSpan="2"
