            <Font name="Arial" size="24"/>
        </font>
    </Label>

    <Label text="Utilities" GridPane.columnIndex="0"
           GridPane.rowIndex="9" >
    </Label>
    <TextField fx:id="tfUtilities" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="9"
               editable="false"/>

    <Label text="Maintenance" GridPane.columnIndex="0"
           GridPane.rowIndex="10" >
    </Label>
    <TextField fx:id="tfMaintenance" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="10"
               editable="false"/>

    <Label text="Repairs" GridPane.columnIndex="0"
           GridPane.rowIndex="11" >
    </Label>
    <TextField fx:id="tfRepairs" prefHeight="40"
               GridPane.columnIndex="1" GridPane.rowIndex="11"
               editable="false"/>

