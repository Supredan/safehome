<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="358.0" prefWidth="865.0" style="-fx-background-color: #d7d7d7;"
            xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.building.BuildingController">
    <children>
        <TableView fx:id="tvBuilding" prefHeight="471.0" prefWidth="690.0" AnchorPane.bottomAnchor="55.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="tfBuildingNo" editable="false" maxWidth="100.0" minWidth="65.0" prefWidth="75.0" text="BuildingNo"/>
                <TableColumn fx:id="tfStreet" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0"
                             prefWidth="200.0" text="Street"/>
                <TableColumn fx:id="tfCity" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0"
                             prefWidth="200.0" text="City"/>
                <TableColumn fx:id="tfState" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0"
                             prefWidth="200.0" text="State"/>
                <TableColumn fx:id="tfZipcode" editable="false" maxWidth="1.7976931348623157E308"
                             minWidth="100.0" prefWidth="100.0" text="Zipcode"/>
                <TableColumn fx:id="tfPhone" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0"
                             prefWidth="100.0" text="Phone"/>
                <TableColumn fx:id="tfEmpNo" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0"
                             prefWidth="100.0" text="EmpNo"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button fx:id="btnReportSubmit" minHeight="25.0" minWidth="88.0"
                mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0" text="Generate Report"
                AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0"/>
        <Button fx:id="btnDeleteBuilding" minHeight="25.0" minWidth="102.0" mnemonicParsing="false" text="DEL"
                textFill="RED" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0"/>
        <Button fx:id="btnEditBuilding" minHeight="25.0" minWidth="94.0" mnemonicParsing="false" text="EDIT"
                AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="132.0"/>
        <Button fx:id="btnAddNewBuilding" minHeight="25.0" minWidth="90.0" mnemonicParsing="false" text="ADD"
                textFill="#048010" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="241.0"/>
    </children>
</AnchorPane>
